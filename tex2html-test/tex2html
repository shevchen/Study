read -r line
cat << EOF
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <title>$line</title>
</head>
<body>
EOF

echo "$(cat)
" |
sed '
  s/.*%.*//
  s/\s\s*/ /g
  s/^\s//
  s/\s$//
  /^\\section{.*}$/ {
    s/^\\section{\(.*\)}$/\<h1\>\1\<\/h1\>/
    x
    s/..*/&\<\/p\>/p
    s/.*//
    x
    p
    d
  }
  /^\\subsection{.*}$/ {
    s/^\\subsection{\(.*\)}$/\<h2\>\1\<\/h2\>/
    x
    s/..*/&\<\/p\>/p
    s/.*//
    x
    p
    d
  }
  /^\s*$/ {
    x
    s/..*/&\<\/p\>/p
    d
  }
  x
  /^\s*$/ {
    x
    s/.*/\<p\>&/
    x
    d
  }
'

echo '</body>'
echo '</html>'
